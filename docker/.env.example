# Copy this file to docker/.env and populate with real values.
DATABASE__URL=postgres://postgres:postgres@postgres:5432/github_spam
API__BIND=0.0.0.0:3000

# GitHub authentication (must be set to real token IDs/secrets).
GITHUB__TOKEN_IDS=tokenA
GITHUB__TOKEN_SECRETS=ghp_exampleA
GITHUB__USER_AGENT=github-spam-lab

# Collector configuration overrides (optional)
COLLECTOR__INTERVAL_SECS=300
COLLECTOR__PAGE_SIZE=100
BROKER__MAX_INFLIGHT=32
BROKER__PER_REPO_INFLIGHT=2
