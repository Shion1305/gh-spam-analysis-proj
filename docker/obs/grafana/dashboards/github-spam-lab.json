{
  "id": null,
  "title": "GitHub Spam Lab",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 3,
  "refresh": "5s",
  "time": { "from": "now-1h", "to": "now" },
  "templating": {
    "list": [
      {
        "name": "repo",
        "type": "query",
        "datasource": "Prometheus",
        "refresh": 1,
        "hide": 0,
        "includeAll": true,
        "allValue": ".*",
        "multi": true,
        "query": "label_values(collector_repository_job_status, repo)",
        "regex": "",
        "current": { "text": "All", "value": [".*"] }
      },
      {
        "name": "budget",
        "type": "query",
        "datasource": "Prometheus",
        "refresh": 1,
        "hide": 0,
        "includeAll": true,
        "allValue": ".*",
        "multi": true,
        "query": "label_values(gh_broker_budget_remaining_total, budget)",
        "current": { "text": "All", "value": [".*"] }
      },
      {
        "name": "fetcher",
        "type": "query",
        "datasource": "Prometheus",
        "refresh": 1,
        "hide": 0,
        "includeAll": true,
        "allValue": ".*",
        "multi": true,
        "query": "label_values(collector_fetch_requests_total, fetcher)",
        "current": { "text": "All", "value": [".*"] }
      },
      {
        "name": "op",
        "type": "query",
        "datasource": "Prometheus",
        "refresh": 1,
        "hide": 0,
        "includeAll": true,
        "allValue": ".*",
        "multi": true,
        "query": "label_values(collector_fetch_requests_total, op)",
        "current": { "text": "All", "value": [".*"] }
      }
    ]
  },
  "panels": [
    {
      "type": "stat",
      "title": "GQL Remaining (by op)",
      "gridPos": { "h": 4, "w": 8, "x": 0, "y": 0 },
      "targets": [ { "expr": "gql_rate_limit_remaining" } ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "values": false } },
      "fieldConfig": { "defaults": { "unit": "none" } }
    },
    {
      "type": "stat",
      "title": "GQL Limit (by op)",
      "gridPos": { "h": 4, "w": 8, "x": 8, "y": 0 },
      "targets": [ { "expr": "gql_rate_limit_limit" } ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "values": false } }
    },
    {
      "type": "timeseries",
      "title": "GQL Remaining over time",
      "gridPos": { "h": 6, "w": 8, "x": 16, "y": 0 },
      "targets": [ { "expr": "gql_rate_limit_remaining", "legendFormat": "{{op}}" } ]
    },
    {
      "type": "timeseries",
      "title": "GQL Resource Limit Events/sec",
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 4 },
      "targets": [ { "expr": "sum by (op) (rate(gql_resource_limit_events_total[5m]))", "legendFormat": "{{op}}" } ]
    },
    {
      "type": "timeseries",
      "title": "DB Issues by Repo",
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 0 },
      "targets": [
        { "expr": "db_issues_total_by_repo{repo=~\"$repo\"}", "legendFormat": "{{repo}}" }
      ]
    },
    {
      "type": "timeseries",
      "title": "DB Comments by Repo",
      "gridPos": { "h": 6, "w": 8, "x": 8, "y": 0 },
      "targets": [
        { "expr": "db_comments_total_by_repo{repo=~\"$repo\"}", "legendFormat": "{{repo}}" }
      ]
    },
    {
      "type": "timeseries",
      "title": "DB Users by Repo",
      "gridPos": { "h": 6, "w": 8, "x": 16, "y": 0 },
      "targets": [
        { "expr": "db_users_total_by_repo{repo=~\"$repo\"}", "legendFormat": "{{repo}}" }
      ]
    },
    {
      "type": "stat",
      "title": "Active Repositories",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "targets": [ { "expr": "collector_active_repositories" } ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "values": false } }
    },
    {
      "type": "stat",
      "title": "Seed Repositories",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 },
      "targets": [ { "expr": "collector_seed_repositories" } ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "values": false } }
    },
    {
      "type": "stat",
      "title": "Runs (total/success/failure/errors)",
      "gridPos": { "h": 4, "w": 12, "x": 12, "y": 0 },
      "targets": [
        { "expr": "collector_runs_total", "legendFormat": "total" },
        { "expr": "collector_run_success_total", "legendFormat": "success" },
        { "expr": "collector_run_failure_total", "legendFormat": "failure" },
        { "expr": "collector_run_errors_total", "legendFormat": "errors" }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "values": false } }
    },
    {
      "type": "timeseries",
      "title": "Broker Rate Remaining",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "targets": [
        {
          "expr": "gh_broker_rate_remaining{budget=~\"$budget\"}",
          "legendFormat": "{{token}}/{{budget}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Broker Pending Requests (queued+inflight)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "targets": [
        {
          "expr": "sum by (budget, priority) (gh_broker_pending_requests{budget=~\"$budget\"})",
          "legendFormat": "{{budget}}/{{priority}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Broker Queue Length",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "targets": [
        {
          "expr": "gh_broker_queue_length{budget=~\"$budget\"}",
          "legendFormat": "{{budget}}/{{priority}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Broker Latency (P95)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(gh_broker_latency_seconds_bucket[5m])) by (le, budget))",
          "legendFormat": "{{budget}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Broker Retry Count",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "targets": [
        {
          "expr": "sum by (budget, reason) (rate(gh_broker_retries_total{budget=~\"$budget\"}[5m]))",
          "legendFormat": "{{budget}}/{{reason}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Cache Hit Ratio",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 20 },
      "targets": [
        {
          "expr": "sum(rate(gh_broker_cache_hits_total[5m])) by (budget) / sum(rate(gh_broker_cache_hits_total[5m]) + rate(gh_broker_cache_misses_total[5m])) by (budget)",
          "legendFormat": "{{budget}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Issues/sec by Repo",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 20 },
      "targets": [
        {
          "expr": "sum by (repo) (rate(collector_issues_processed_total{repo=~\"$repo\"}[5m]))",
          "legendFormat": "{{repo}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Comments/sec by Repo",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 28 },
      "targets": [
        {
          "expr": "sum by (repo) (rate(collector_comments_processed_total{repo=~\"$repo\"}[5m]))",
          "legendFormat": "{{repo}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Repo Processing Duration (P95)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 28 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(collector_repository_duration_seconds_bucket[5m])) by (le, outcome))",
          "legendFormat": "{{outcome}}"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Jobs: Pending",
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 36 },
      "targets": [ { "expr": "sum(collector_repository_job_status{status=\"pending\",repo=~\"$repo\"})" } ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "values": false } }
    },
    {
      "type": "stat",
      "title": "Jobs: In Progress",
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 36 },
      "targets": [ { "expr": "sum(collector_repository_job_status{status=\"in_progress\",repo=~\"$repo\"})" } ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "values": false } }
    },
    {
      "type": "stat",
      "title": "Jobs: Completed",
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 36 },
      "targets": [ { "expr": "sum(collector_repository_job_status{status=\"completed\",repo=~\"$repo\"})" } ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "values": false } }
    },
    {
      "type": "stat",
      "title": "Jobs: Failed",
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 36 },
      "targets": [ { "expr": "sum(collector_repository_job_status{status=\"failed\",repo=~\"$repo\"})" } ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "values": false } }
    },
    {
      "type": "stat",
      "title": "Jobs: Error",
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 36 },
      "targets": [ { "expr": "sum(collector_repository_job_status{status=\"error\",repo=~\"$repo\"})" } ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "values": false } }
    },
    {
      "type": "bargauge",
      "title": "In-Progress Repositories",
      "gridPos": { "h": 8, "w": 8, "x": 20, "y": 36 },
      "targets": [
        { "expr": "collector_repository_job_status{status=\"in_progress\",repo=~\"$repo\"}", "legendFormat": "{{repo}}" }
      ]
    },
    {
      "type": "bargauge",
      "title": "Job Failures by Repo",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 40 },
      "targets": [
        { "expr": "collector_repository_job_failure_count", "legendFormat": "{{repo}}" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Last Attempt Age (minutes) by Repo",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 40 },
      "targets": [
        { "expr": "(time() - collector_repository_last_attempt_timestamp_seconds{repo=~\"$repo\"}) / 60", "legendFormat": "{{repo}}" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Last Success Age (minutes) by Repo",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 48 },
      "targets": [
        { "expr": "(time() - collector_repository_last_success_timestamp_seconds{repo=~\"$repo\"}) / 60", "legendFormat": "{{repo}}" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Users Fetched/sec",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 48 },
      "targets": [
        { "expr": "rate(collector_users_fetched_total[5m])" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Fetcher Requests/sec (by fetcher/op/outcome)",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 56 },
      "targets": [
        { "expr": "sum by (fetcher, op, outcome) (rate(collector_fetch_requests_total{fetcher=~\"$fetcher\",op=~\"$op\"}[5m]))", "legendFormat": "{{fetcher}}/{{op}}/{{outcome}}" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Fetcher Items/sec (by fetcher/op)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 64 },
      "targets": [
        { "expr": "sum by (fetcher, op) (rate(collector_fetch_items_total{fetcher=~\"$fetcher\",op=~\"$op\"}[5m]))", "legendFormat": "{{fetcher}}/{{op}}" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Fetcher Latency P95 (by fetcher/op)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 64 },
      "targets": [
        { "expr": "histogram_quantile(0.95, sum(rate(collector_fetch_latency_seconds_bucket{fetcher=~\"$fetcher\",op=~\"$op\"}[5m])) by (le, fetcher, op))", "legendFormat": "{{fetcher}}/{{op}}" }
      ]
    },
    {
      "type": "stat",
      "title": "REST Budget Remaining (total)",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 72 },
      "targets": [ { "expr": "gh_broker_budget_remaining_total{budget=\"core\"}" } ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "values": false } }
    },
    {
      "type": "stat",
      "title": "GraphQL Budget Remaining (total)",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 72 },
      "targets": [ { "expr": "gh_broker_budget_remaining_total{budget=\"graphql\"}" } ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "values": false } }
    },
    {
      "type": "gauge",
      "title": "REST Budget Utilization",
      "gridPos": { "h": 6, "w": 6, "x": 12, "y": 72 },
      "targets": [ { "expr": "(gh_broker_budget_remaining_total{budget=\"core\"} / gh_broker_budget_limit_total{budget=\"core\"}) * 100" } ],
      "fieldConfig": { "defaults": { "unit": "percent" } }
    },
    {
      "type": "gauge",
      "title": "GraphQL Budget Utilization",
      "gridPos": { "h": 6, "w": 6, "x": 18, "y": 72 },
      "targets": [ { "expr": "(gh_broker_budget_remaining_total{budget=\"graphql\"} / gh_broker_budget_limit_total{budget=\"graphql\"}) * 100" } ],
      "fieldConfig": { "defaults": { "unit": "percent" } }
    }
  ]
}
